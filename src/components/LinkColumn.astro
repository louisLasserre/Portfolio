---
import type { TLinkType } from "./button.astro";

interface Props {
  title: string;
  links: TLinkType[];
}

const { title, links } = Astro.props;
---

<div class="flex flex-col gap-2 my-6">
  <p class="font-button inline-block mb-2">{title}</p>
  {
    links.map((link) => (
      <a class="font-text block" href={link.url}>
        {link.name}
      </a>
    ))
  }
</div>
